{
  "name": "roblox-editor",
  "displayName": "Roblox Editor",
  "description": "Allows you to edit Roblox scrips outside of Roblox studio.",
  "icon": "icons/icon.png",
  "publisher": "adamfoster",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "roblox-editor.openPlace",
        "title": "Open Place"
      },
      {
        "command": "roblox-editor.editPlaces",
        "title": "Edit Places"
      },
      {
        "command": "roblox-editor.loadChildren",
        "title": "Load Children"
      },
      {
        "command": "roblox-editor.openEditor",
        "title": "Open Editor"
      },
      {
        "command": "roblox-editor.saveFile",
        "title": "Save File To Roblox"
      },
      {
        "command": "roblox-editor.saveAllFiles",
        "title": "Save All Files To Roblox"
      }
    ],
    "keybindings": [
      {
        "command": "roblox-editor.saveFile",
        "key": "alt+d",
        "mac": "alt+d",
        "when": "editorTextFocus"
      },
      {
        "command": "roblox-editor.saveAllFiles",
        "key": "alt+k d",
        "mac": "alt+k d",
        "when": "editorTextFocus"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "explorer",
          "name": "Explorer"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/afoster549/roblox-editor"
  },
  "author": "Adam Foster",
  "license": "MIT"
}
